
.chartsdom {
	position: relative;
}
.charts-error {
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	background-color: rgba(255, 255, 255, 0.8);
	z-index: 998;
}
.charts-error-img {
	width: 80px;
	height: 80px;
}
.charts-error-text {
	margin-top: 10px;
	font-size: 12px;
	color: #909399;
}

